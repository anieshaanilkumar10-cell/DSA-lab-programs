#include <stdio.h>
#include <stdlib.h>
void TOH(int n,char s,char t,char d)
{
    if(n>0)
{
    TOH(n-1,s,t,d);
    printf("\nMove %d disc from%c to %c",n,s,d);
    TOH(n-1,t,d,s);
}
}
int main()
{
    int n;
    printf("\nRead no of discs:");
    scanf("%d",&n);
    TOH(n,'S','D','T');
    return 0;
}

